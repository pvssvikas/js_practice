extends layout


block content
    .container-fluid
        .row
            .page-header.header_site
                .clearfix
                    .pull-left
                        img(src='/images/product-home.png' style='height:30px')
                    .pull-left
                        h1 Home
    .container
        .row    
            .col-md-4.col-sm-4
                .panel.panel-primary.first
                    .panel-header
                    .panel-body
                        #treeview
                            ul
                                li Switch Board Definitions
                                    ul
                                        li Home Appliances
                                            ul 
                                                li General Appliances
                                                    ul
                                                        li Fan
                                                        li Light
                                                        li AC
                                                        li Cooler
                                                li Kitchen
                                                    ul
                                                        li Fridge
                                                        li Grinder
                                                        li Exhaust
                                                li Entertainment
                                                    ul
                                                        li TV   
                                                        li Music System
                                        li Power Sockets
            .col-md-6.col-sm-6
                .panel.panel-primary.first
                    .panel-header
                    .panel-body
                        .col-md-4.col-md-offset-6.col-sm-4.col-sm-offset-6.col-xs-4.col-xs-offset-6
                            input(type='text' placeholder='switch board name') 
                        
                .col-xs-4.col-xs-offset-6.col-sm-4.col-sm-6.col-md-4.col-md-6
                    a(href='/defineHome' class='btn btn-primary') Done
